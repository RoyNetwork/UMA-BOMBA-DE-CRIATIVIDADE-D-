CREATE TABLE Livros (
 id int,
 titulo varchar (45), 
 autor varchar (45),
 editora varchar (45),
 ano int (45), 
 status bool
 );
 
CREATE TABLE Usuarios (
id int, 
nome varchar (45),
 email varchar (45),
 telefone varchar (45)
 );
 
CREATE TABLE Emprestimos (
 id int,
 livro_id int,
 usuario_id int,
 data_emprestimo date,
 data_devolucao date, 
 status bool
 );
 
 
